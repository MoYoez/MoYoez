#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/markdown-it@12.3.0/node_modules/markdown-it/bin/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/markdown-it@12.3.0/node_modules/markdown-it/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/markdown-it@12.3.0/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules:/home/runner/work/MoYoez/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/markdown-it/bin/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/markdown-it/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../markdown-it/bin/markdown-it.js" "$@"
else
  exec node  "$basedir/../markdown-it/bin/markdown-it.js" "$@"
fi
