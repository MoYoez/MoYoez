#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/typescript@4.4.3/node_modules/typescript/bin/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/typescript@4.4.3/node_modules/typescript/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/typescript@4.4.3/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules:/home/runner/work/MoYoez/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/ts-node@10.3.0_qmcozvyvqmhxygiljuo6vefraa/node_modules/typescript/bin/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/ts-node@10.3.0_qmcozvyvqmhxygiljuo6vefraa/node_modules/typescript/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/ts-node@10.3.0_qmcozvyvqmhxygiljuo6vefraa/node_modules:/home/runner/work/MoYoez/MoYoez/node_modules/.pnpm/ts-node@10.3.0_qmcozvyvqmhxygiljuo6vefraa/node_modules/ts-node/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../typescript/bin/tsserver" "$@"
fi
